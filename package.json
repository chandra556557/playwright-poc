{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/index.js",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "vite build",
    "build:client": "vite build",
    "build:server": "echo 'Server build not required'",
    "start": "node server/index.js",
    "start:test": "NODE_ENV=test node server/index.js",
    "lint": "eslint .",
    "lint:server": "eslint server/",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    
    "test:unit": "vitest run src/ server/",
    "test:unit:watch": "vitest src/ server/ --watch",
    "test:unit:ui": "vitest src/ server/ --ui",
    
    "test:integration": "vitest run server/__tests__/integration/",
    "test:integration:watch": "vitest server/__tests__/integration/ --watch",
    
    "test:coverage": "vitest run --config=vitest.coverage.config.js",
    "test:coverage:ui": "vitest --config=vitest.coverage.config.js --ui",
    "test:coverage:watch": "vitest --config=vitest.coverage.config.js --watch",
    
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:allure": "playwright test --reporter=allure-playwright",
    "test:e2e:critical": "playwright test tests/e2e-critical-journeys.spec.js",
    "test:e2e:demo": "playwright test --config=playwright.e2e-demo.config.js",
    "test:e2e:demo:headed": "playwright test --config=playwright.e2e-demo.config.js --headed",
    "test:e2e:demo:debug": "playwright test --config=playwright.e2e-demo.config.js --debug",
    "test:e2e:demo:ui": "playwright test --config=playwright.e2e-demo.config.js --ui",
    
    "test:performance": "playwright test tests/performance/",
    "test:performance:headed": "playwright test tests/performance/ --headed",
    "test:load": "artillery run tests/performance/load-test.yml",
    
    "test:visual": "percy exec -- playwright test --config=playwright.visual.config.js",
    "test:visual:update": "percy exec -- playwright test --config=playwright.visual.config.js --update-snapshots",
    "test:visual:debug": "percy exec -- playwright test --config=playwright.visual.config.js --debug",
    "test:visual:headed": "percy exec -- playwright test --config=playwright.visual.config.js --headed",
    "test:visual:ui": "percy exec -- playwright test --config=playwright.visual.config.js --ui",
    
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e && npm run test:performance",
    "test:quick": "npm run test:unit && npm run test:integration",
    
    "install:server": "cd server && npm install",
    
    "percy:start": "percy app:create",
    "percy:finalize": "percy build:finalize",
    "percy:status": "percy build:status",
    
    "allure:generate": "allure generate allure-results --clean",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    
    "coverage:open": "open coverage/index.html",
    "coverage:serve": "npx http-server coverage -p 8080",
    
    "clean": "rm -rf dist coverage test-results allure-results allure-report playwright-report",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "deps:check": "npx depcheck",
    "deps:update": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@playwright/test": "^1.55.0",
    "@tanstack/react-query": "^5.87.1",
    "@tensorflow/tfjs": "^4.22.0",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "brain.js": "^2.0.0-beta.23",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jimp": "^0.22.12",
    "lucide-react": "^0.344.0",
    "ml-matrix": "^6.10.7",
    "multer": "^2.0.2",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.55.0",
    "pngjs": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.0",
    "sharp": "^0.33.2",
    "similarity": "^1.2.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@percy/cli": "^1.31.2",
    "@percy/playwright": "^1.0.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.1",
    "allure-commandline": "^2.24.1",
    "allure-playwright": "^3.0.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "jsdom": "^25.0.1",
    "supertest": "^7.0.0",
    "artillery": "^2.0.20",
    "ws": "^8.18.3",
    "@types/supertest": "^6.0.2"
  }
}
