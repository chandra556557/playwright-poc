{
  "name": "playwright-testing-suite",
  "version": "1.0.0",
  "description": "Comprehensive Playwright testing suite with self-healing capabilities",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "node server/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:allure": "allure serve allure-results",
    "test:allure:generate": "allure generate allure-results --clean -o allure-report",
    "test:e2e-demo": "playwright test --config=playwright.e2e-demo.config.js",
    "test:critical": "playwright test tests/e2e-critical-journeys.spec.js",
    "test:blazemeter": "playwright test tests/blazemeter-ecommerce-scenarios.spec.js",
    "test:simple": "playwright test tests/simple-test.spec.js",
    "start": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "start:ai": "node start-with-ai-inspector.js",
    "start:ai-inspector": "cd ai-element-inspector-js && node start-integrated-server.js",
    "start:frontend": "npm run dev",
    "start:backend": "npm run dev:server",
    "install:browsers": "playwright install",
    "install:deps": "playwright install --with-deps",
    "clean": "rimraf test-results allure-results allure-report playwright-report",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@playwright/test": "^1.55.0",
    "@tanstack/react-query": "^5.87.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "brain.js": "^2.0.0-beta.23",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jimp": "^0.22.12",
    "lucide-react": "^0.344.0",
    "ml-matrix": "^6.10.7",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.55.0",
    "pngjs": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.2.0",
    "sharp": "^0.33.2",
    "similarity": "^1.2.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@percy/cli": "^1.31.2",
    "@percy/playwright": "^1.0.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.1",
    "allure-commandline": "^2.24.1",
    "allure-playwright": "^3.0.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
